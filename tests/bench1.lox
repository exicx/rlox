var start;

fun start_bench() {
  print "Starting benchmark.";
  start = clock();
}

fun end_bench() {
  print "Ending benchmark.";
  var result = clock() - start;
  print result;
  print "seconds";
}

fun fib(limit) {
  var a = 0;
  var temp;

  for (var b = 1; a < limit; b = temp + b) {
    temp = a;
    a = b;
  }
}

fun bench() {
  start_bench();
  for (var i = 0; i < 1000; i = i + 1) {
    for (var j = 0; j < 1000; j = j + 1) {
      fib(10000);
    }
  }
  end_bench();
}

bench();

